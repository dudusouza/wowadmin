#AddHandler php53-script .php .php5 .php53 .pht .phtm .phtml
#suPHP_ConfigPath /home/loginFtp/public_html/

RewriteEngine On

#
#Adiciona o ao dominio www.*
#
#RewriteCond %{HTTP_HOST} !^www\.
#RewriteRule ^(.*)$ http://www.%{HTTP_HOST}/$1 [R=301,L]

RewriteCond %{REQUEST_FILENAME} !-f
RewriteCond %{REQUEST_FILENAME} !-d
RewriteRule ^(.*)$ index.php [NC,QSA]

#Remove o acesso a pasta protected
RewriteRule ^(?:protected)\b.* index.php

#Desabilita acesso sem extensão
Options -MultiViews
#
#Compacta��o Ativa
#
RewriteCond %{HTTP_USER_AGENT} !".*Safari.*"
RewriteCond %{HTTP:Accept-encoding} gzip
RewriteCond %{REQUEST_FILENAME}.gz -f
RewriteRule ^(.*)$ $1.gz [QSA,L]
<FilesMatch \.css\.gz$>
  ForceType text/css
</FilesMatch>
<FilesMatch \.js\.gz$>
  ForceType application/javascript
</FilesMatch>
<IfModule mod_mime.c>
  AddEncoding gzip .gz
</IfModule>

SetOutputFilter DEFLATE